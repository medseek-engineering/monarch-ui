{
  "name": "monarch-ui",
  "version": "1.0.0",
  "uiVersion": "1.0.0",
  "description": "monarch-ui",
  "author": "Influence Health",
  "dependencies": {
    "browser-sync": "^2.8.2",
    "ui-core": "^1.0.0",
    "iui-general": "git+ssh://git@github.com:medseek-engineering/iui-general.git",
    "iui-lists": "git+ssh://git@github.com:medseek-engineering/iui-lists.git",
    "iui-table": "git+ssh://git@github.com:medseek-engineering/iui-table.git",
    "iui-alerts": "git+ssh://git@github.com:medseek-engineering/iui-alerts.git",
    "longitudinal-view": "git+ssh://git@github.com:medseek-engineering/iui-alerts.git",
    "lodash": "~2.4.1",
    "d3": "^3.5.14",
    "delayed-stream": "0.0.5",
    "express": "^4.13.3",
    "gulp": "^3.9.0",
    "gulp-compass": "^2.1.0",
    "gulp-concat": "~2.1.7",
    "gulp-filter": "^3.0.0",
    "gulp-hologram": "^1.2.0",
    "gulp-html2js": "^0.2.0",
    "gulp-jshint": "^1.11.2",
    "gulp-minify-css": "^1.2.0",
    "gulp-ng-html2js": "^0.2.0",
    "gulp-nodemon": "^2.0.6",
    "gulp-rename": "~0.2.2",
    "gulp-uglify": "~0.2.0",
    "gulp-util": "~2.2.12",
    "jquery": "~2.1.4",
    "jshint": "~2.3.0",
    "jshint-stylish": "~1.0.0",
    "karma": "~0.12.1",
    "karma-chrome-launcher": "~0.1.2",
    "karma-coverage": "~0.1",
    "karma-firefox-launcher": "~0.1.3",
    "karma-html-reporter": "~0.1.1",
    "karma-html2js-preprocessor": "~0.1.0",
    "karma-jasmine": "~0.2.2",
    "karma-ng-html2js-preprocessor": "~0.1.0",
    "karma-requirejs": "~0.2.0",
    "karma-spec-reporter": "0.0.6",
    "karma-phantomjs-launcher": "^1.0.0",
    "phantomjs-prebuilt": "^2.1.4"
  },
  "devDependencies": {
    "medseek-config": "^0.0.1"
  },
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/medseek-engineering/monarch-ui.git"
  },
  "engines": {
    "node": ">=0.10.21"
  },
  "gulpSettings": {
    "base": "./app/",
    "styleModules": [
      "iui-general",
      "iui-lists",
      "iui-table",
      "longitudinal-view"
    ],
    "compass": {
      "sass": "app/themes/default_sass",
      "css": "app/themes/default",
      "require": []
    },
    "appFiles": {
      "styles": [
        "./app/themes/default_sass/**/*.scss",
        "./app/themes/default_sass/*.scss"
      ],
      "htmlFiles": [
        "./app/**/*.html",
        "./app/**/*.js"
      ],
      "jsFiles": [
        "./app/**/*.js"
      ]
    },
    "destination": {
      "css": "./app/themes/default"
    }
  },
  "scripts": {
    "start": "node app.js",
    "postinstall": "gulp build"
  }
}
